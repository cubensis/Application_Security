mv /var/backup/mysql/mysql_all.dmp.8 /var/backup/mysql/mysql_all.dmp.9 &&
mv /var/backup/mysql/mysql_all.dmp.7 /var/backup/mysql/mysql_all.dmp.8 &&
mv /var/backup/mysql/mysql_all.dmp.6 /var/backup/mysql/mysql_all.dmp.7 &&
mv /var/backup/mysql/mysql_all.dmp.5 /var/backup/mysql/mysql_all.dmp.6 &&
mv /var/backup/mysql/mysql_all.dmp.4 /var/backup/mysql/mysql_all.dmp.5 &&
mv /var/backup/mysql/mysql_all.dmp.3 /var/backup/mysql/mysql_all.dmp.4 &&
mv /var/backup/mysql/mysql_all.dmp.2 /var/backup/mysql/mysql_all.dmp.3 &&
mv /var/backup/mysql/mysql_all.dmp.1 /var/backup/mysql/mysql_all.dmp.2 &&
mv /var/backup/mysql/mysql_all.dmp /var/backup/mysql/mysql_all.dmp.1 &&
mysqldump -u root -pbigGoat -r /var/backup/mysql/mysql_all.dmp -A
echo "MySQL Backup Run Done `ls -l /var/backup/mysql/`"

